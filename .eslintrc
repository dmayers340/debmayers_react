{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": ["react"],
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "no-console": "off",
        "strict": ["error", "global"],
        "curly": "warn"
    }
}