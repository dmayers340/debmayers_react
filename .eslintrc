{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
        "jsx-a11y",
        "react"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "no-console": "off",
        "strict": ["error", "global"],
        "curly": "warn"
    }
}